<main>
  <div class="row">
    <section id="1">
      <div class="row">
        {{#with navigation}}
        <div class="card">
          <div class="nav">
            {{#each links}}
            <div class="nav-item m-2">
              <a href="{{href}}" class="nav-link active {{#if @last includeZero=true}} disabled {{/if}}">{{text}}</a>
            </div>
            {{/each}}
          </div>
        </div>
        {{/with}}
      </div>
    </section>

    <section id="2">
      <h1 class="my-5">{{header}}</h1>
      {{#with contacts_data}}
      <div class="row">
        <div class="col-8">
          <div class="row">


            <h2 class="col-12 my-3">{{title}}</h2>
            <div class="row">
              {{#each buttons}}
              <div class="col-6 my-2">
                <a
                  class="btn {{#if isPrimary}}btn-warning text-light {{/if}} {{#if isOutline}} btn-outline-warning {{/if}} px-5">{{text}}</a>
              </div>
              {{/each}}


              {{#with form}}
              {{#each input}}
              <div class="col-6 my-4">
                <label class="form-label">{{label}}</label>
                <input type="text" class="form-control" placeholder="{{placeholder}}">
                <div class="form-text text-danger">{{caption}}</div>
              </div>
              {{/each}}

              <hr class="my-4">

              {{#each switch}}
              <div class="col-8 my-2">
                <div class="form-check form-switch">
                  <label class="form-check-label">{{text}}</label>
                  <input class="form-check-input" type="checkbox" {{#unless isActive}} disabled {{/unless}}>
                </div>
              </div>
              {{/each}}
              {{/with}}
            </div>
          </div>
          {{/with}}

        </div>

        <div class="col-4 my-3">

          <div class="p-2">
            <div class="card">
              <div class="card-body">
                {{#each action_block}}
                <a
                  class="btn {{#if isSuccess}}btn-success{{/if}} {{#if isDanger}} btn-danger{{/if}} {{#if isInfo}} btn-info text-light{{/if}} my-2 pe-5">{{text}}</a>
                {{/each}}
              </div>
            </div>
          </div>

          {{#with total}}
          <div class="mt-3 p-2">
            <div class="card pe-3">
              <div class="card-body">
                <h3 class="card-title">{{title}}</h3>
                {{#each item_list}}
                <div class="card-text">{{name}}: {{price}}</div>
                {{/each}}
                <hr>
                {{#each additional_info}}
                <div class="card-text">{{text}}: {{value}}</div>
                {{/each}}
                <hr>
                <div class="card-text">{{total_price.text}}: {{total_price.value}}</div>
                <hr>
                {{#each button}}
                <a
                  class="btn {{#if isPrimary}} btn-primary{{/if}} {{#if isOutline}}btn-outline-primary{{/if}} my-2">{{text}}</a>
                {{/each}}
              </div>
            </div>
          </div>
          {{/with}}
        </div>
      </div>
    </section>


    <hr class="my-3">

    <section id="3">
      <div col="row">

        <div class="group-1 col-8">
          {{#with delivery}}
          <h2 class="my-3">{{title}}</h2>


          <div class="card my-4">
            <div class="card-body">
              <div class="row">
                {{#each select}}
                <div class="col-6">
                  <div class="card-title">
                    <div class="form-label">{{label}}</div>
                  </div>

                  <select class="form-select">
                    <option selected>{{placeholder}}</option>
                    {{#each options}}
                    <option value="{{value}}">{{text}}</option>
                    {{/each}}
                  </select>
                </div>
                {{/each}}

              </div>
            </div>
          </div>


          {{#each delivery_type}}
          <div class="form-check my-2">
            <input class="form-check-input" type="radio" name="type">
            <label class="form-check-label">{{type}}</label>
          </div>
          {{/each}}

          <hr class="my-4">

          {{#with payment}}
          <h2 class="my-3">{{title}}</h2>

          {{#each list}}
          <div class="form-check my-3">
            <input class="form-check-input" type="radio" name="type">
            <label>{{type}}</label>
            <div class="form-text">
              {{description}}
            </div>
          </div>
          {{/each}}
          {{/with}}

          <hr class="my-3">

          <h3>{{title}}</h3>
          {{#with contact}}

          <div class="row">
            {{#each this}}
            <div class="col-6">
              <label class="form-label">{{label}}</label>
              <input type="text" class="form-control" placeholder="{{placeholder}}">
              <div class="form-text text-danger">{{caption}}</div>
            </div>
            {{/each}}
          </div>
          {{/with}}

          <hr class="my-3">

          {{#with coment}}
          <label class="form-label">{{label}}</label>
          <input type="text" class="form-control" placeholder="{{placeholder}}">
          {{/with}}

          <hr class="my-4">

          {{/with}}
        </div>

        <div class="group-2 col-8">

          <div class="coupon">
            {{#with coupon}}
            <h3>{{title}}</h3>

            {{#with select.availabel_coupon}}
            <div class="text-muted my-3">{{label}}</div>
            <select class="form-select p-3">
              <option selected>{{placeholder}}</option>
              {{#each options}}
              <option value="{{value}}">{{text}}</option>
              {{/each}}
            </select>
            {{/with}}

            {{#with select.friend_coupon}}
            <div class="text-muted my-3">{{label}}</div>
            <select class="form-select p-3">
              <option selected>{{placeholder}}</option>
              {{#each options}}
              <option value="{{value}}">{{text}}</option>
              {{/each}}
            </select>
            {{/with}}

            <div class="my-3">
              {{#with add}}

              {{#with friend}}
              <label class="form-label text-muted">{{label}}</label>
              <input class="form-control p-3" placeholder="{{placeholder}}">
              <div class="form-text">{{caption}}</div>
              {{/with}}

              {{#with colleague}}
              <label class="form-label text-muted mt-4">{{label}}</label>
              <input class="form-control p-3" placeholder="{{placeholder}}">
              <div class="form-text">{{caption}}</div>
              {{/with}}

              {{/with}}
            </div>

            {{/with}}
          </div>

          <hr class="my-4">

          <div class="quiz">
            {{#with quiz}}
            <h4>{{title}}</h4>

            {{#with preference}}
            <label class="form-label">{{title}}</label>
            {{#each options}}
            <div class="form-check ">
              <label class="form-check-label ">
                {{this}}
              </label>
              <input class="form-check-input" type="checkbox">
            </div>
            {{/each}}
            {{/with}}


            <label class="form-label">{{your_expirience.label}}</label>
            <input class="form-control" type="text">

            <div>
              {{#with purchase_frequency}}
              <div class=>{{label}}</div>
              <select class="form-select">
                <option selected>{{placeholder}}</option>
                {{#each options}}
                <option value="{{value}}">{{text}}</option>
                {{/each}}
                {{/with}}

            </div>
            <div>TEST</div>
            <div>
              <p>НІЧОГО НЕ ВИВОДИТЬСЯ</p>
              {{#with factors}}
              <label class="form-label">{{title}}</label>
              {{#each options}}
              <div class="form-check">
                <label class="form-check-label">
                  {{this}}
                </label>
                <input class="form-check-input" type="checkbox">
              </div>
              {{/each}}
              {{/with}}
            </div>
            оололол

            {{/with}}
          </div>

        </div>








      </div>
    </section>





  </div>
</main>