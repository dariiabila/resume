<div>
  <main>
    <h1>{{name}}</h1>
    <dfn>{{description}}</dfn>
    <br>
    <hr>
    <h2>History</h2>
    {{#with history}}
    <p>Year: {{year}}</p>
    <p>Founder: {{founder}}</p>
    {{/with}}
    <h3>Types</h3>
    <ul>
      {{#each types}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <hr>
    <h3>Syntax:</h3>
    {{#with syntax}}
    <p>Variables:</p>
    <ul>
      {{#each variables}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p>Functions:</p>
    <ul>
      {{#each functions}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p>Conditionals:</p>
    <ul>
      {{#each conditionals}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p>Loops:</p>
    <ul>
      {{#each loops}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p>Classes:</p>
    <ul>
      {{#each classes}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    {{/with}}
    <hr>
    <h3>Features</h3>
    <ol>
      {{#each features}}
      <li>{{this}}</li>
      {{/each}}
    </ol>
    <hr>

    <h3>Libraries: </h3>
    <ul>
      {{#each libraries}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <hr>
    <h3>Tools:</h3>
    <ul>
      {{#each tools}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <hr>
    <h4>Community: </h4>
    <ul>
      {{#each community}}
      <li>{{name}}, {{type}}</li>
      {{/each}}
    </ul>

  </main>
</div>